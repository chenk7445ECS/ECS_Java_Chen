public class UsingVariables 
	{
		public static void main(String[] args )
		{
			int variable1;
			double variable2;
			String variable3;
			
			variable1 = 113;
			variable2 = 2.7182;
			variable3 = Computer Science;
			
			System.out.println( "This is room" + "variable1");
			System.out.println("e is close to" +  "variable2 ");
			System.out.println("I am learning a bit about" + "variable3");
			
					
			
			
		}



