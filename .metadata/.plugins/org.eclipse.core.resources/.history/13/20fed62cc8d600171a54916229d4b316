public class UsingVariables 
	{
		public static void main(String[] args )
		{
			int variable1;
			double variable2;
			String variable3;
			
			variable1 = 113;
			variable2 = 2.7182;
			variable3 = "Computer_Science";
			
			System.out.println( "This is room" + " 113");
			System.out.println("e is close to" +  "2.7182 ");
			System.out.println("I am learning a bit about" +"Computer_Science");
			
					
}
	}



