public class YourInitials 
{
	public static void main( String[] args )
	{
		System.out.println("K      K                 C");
		System.out.println("K    K                 C");
		System.out.println("K  K                 C");
		System.out.println("K                  C");
		System.out.println("K  K                 C");
		System.out.println("K    K                 C" );
		System.out.println("K      K                  C'" );
	}
}
